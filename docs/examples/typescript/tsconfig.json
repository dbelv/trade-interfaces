{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "paths": {
      "@/gen/*": ["./gen/*"],
    },
    "types": ["node"],
  },
  "include": [
    "docs/examples/typescript/**/*.ts",
  ],
  "exclude": ["node_modules", "gen"]
}